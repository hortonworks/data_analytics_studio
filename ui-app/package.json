{
  "name": "hivestudio",
  "version": "0.0.1",
  "description": "Hive Studio UI",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "TMPDIR=tmp node ./node_modules/ember-cli/bin/ember build --output-path=dist/assets",
    "start": "TMPDIR=tmp node ./node_modules/ember-cli/bin/ember server --output-path=dist/assets",
    "test": "TMPDIR=tmp node ./node_modules/ember-cli/bin/ember test --output-path=dist/assets",
    "test-serve": "TMPDIR=tmp node ./node_modules/ember-cli/bin/ember test --serve --output-path=dist/assets",
    "build:mvn": "TMPDIR=tmp node/node ./node_modules/ember-cli/bin/ember build --output-path=dist/assets",
    "test:mvn": "TMPDIR=tmp node/node ./node_modules/ember-cli/bin/ember test --output-path=dist/assets",
    "uilicense": "TMPDIR=tmp node/node ./node_modules/ux-license-report/cli.js ./ --include 'npm, dev, bower'  --template ./templatelicence.txt > thirdparty_licence_ui.txt"
  },
